{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/socket"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}